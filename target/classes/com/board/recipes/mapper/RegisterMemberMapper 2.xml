<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.board.recipes.mapper.RegisterMemberMapper">

    <insert id="insertMemberInfo" parameterType="com.board.recipes.entity.MemberRegistrationEntity">
        INSERT INTO
            USER(NAME, PASSWORD)
        VALUES(
            #{name},
            #{password})
    </insert>

</mapper>
